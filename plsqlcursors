Â· Parameterised Cursor :

declare
eid int;
en varchar2(20);
sal float;
dept varchar2(20);
nm varchar2(20) := 'it';
cursor c1(p_dept varchar2) is
select eid, en, sal, dept
from employee
where dept = p_dept;
begin
open c1(nm);
loop
fetch c1 into eid, en, sal, dept;
exit when c1%notfound;
dbms_output.put_line('eid: ' || to_char(eid) || ', en: ' || en || ', sal: ' || to_char(sal) || ', dept: ' || dept);
end loop;
close c1;
end;
/


declare
eid int;
en varchar(20);
sal float;
dept varchar(20);
nm varchar(20);
cursor c1 is select * from employe;
begin
open c1;
loop
fetch c1 into eid,en,sal,dept;
exit when c1%notfound;
dbms_output.put_line(eid || en || sal || dept);
end loop;
close c1;
end;
/

